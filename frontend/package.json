{
    "name": "tsugou-kun-react-fe",
    "version": "1.0.0",
    "description": "都合くん（スケジュール調整アプリ）のフロントエンド実装",
    "main": "index.js",
    "scripts": {
        "test": "jest --env=jsdom --verbose --watchAll",
        "dev": "webpack",
        "prod": "cross-env NODE_ENV=production webpack",
        "watch": "webpack -w",
        "start": "webpack-dev-server",
        "storybook": "start-storybook -p 6006 --no-dll -s src/main",
        "build-storybook": "build-storybook --no-dll",
        "codegen": "graphql-codegen --config graphql-codegen.yml"
    },
    "keywords": [
        "tsugou-kun"
    ],
    "author": "isoittech",
    "license": "ISC",
    "devDependencies": {
        "@babel/core": "^7.12.3",
        "@babel/preset-env": "^7.12.1",
        "@babel/preset-react": "^7.12.1",
        "@babel/preset-typescript": "^7.12.1",
        "@graphql-codegen/cli": "^1.19.4",
        "@graphql-codegen/typescript": "^1.19.0",
        "@graphql-codegen/typescript-operations": "^1.17.12",
        "@graphql-codegen/typescript-react-apollo": "^2.2.1",
        "@storybook/addon-actions": "^6.1.2",
        "@storybook/addon-essentials": "^6.1.2",
        "@storybook/addon-links": "^6.1.2",
        "@storybook/react": "^6.1.2",
        "@testing-library/jest-dom": "^5.11.5",
        "@testing-library/react": "^11.1.1",
        "@testing-library/user-event": "^12.2.0",
        "@types/jest": "^26.0.15",
        "@types/react": "^16.9.53",
        "@types/react-dom": "^16.9.8",
        "@types/react-redux": "^7.1.9",
        "@types/react-router-dom": "^5.1.6",
        "babel-jest": "^26.6.1",
        "babel-loader": "^8.2.1",
        "babel-preset-react-app": "^9.1.2",
        "clean-webpack-plugin": "^3.0.0",
        "cross-env": "^7.0.2",
        "css-loader": "^4.3.0",
        "enzyme": "^3.11.0",
        "enzyme-adapter-react-16": "^1.15.5",
        "html-webpack-plugin": "^4.5.0",
        "jest-environment-jsdom-fourteen": "^1.0.1",
        "mini-css-extract-plugin": "^1.2.1",
        "react-is": "^17.0.1",
        "react-test-renderer": "^17.0.1",
        "redux-saga": "^1.1.3",
        "style-loader": "^1.3.0",
        "ts-jest": "^26.4.3",
        "ts-loader": "^8.0.6",
        "ts-node": "^9.1.1",
        "tslint": "^6.1.3",
        "typescript": "^4.0.3",
        "webpack": "^4.44.2",
        "webpack-cli": "^3.3.12",
        "webpack-dev-server": "^3.11.0"
    },
    "dependencies": {
        "@apollo/client": "^3.3.4",
        "@material-ui/core": "^4.11.2",
        "@material-ui/styles": "^4.11.2",
        "@reduxjs/toolkit": "^1.4.0",
        "@types/moment": "^2.13.0",
        "@types/winston": "^2.4.4",
        "axios": "^0.20.0",
        "bootstrap": "^4.5.2",
        "dotenv": "^8.2.0",
        "graphql": "^15.4.0",
        "history": "^5.0.0",
        "moment": "^2.29.1",
        "react": "^16.13.1",
        "react-apollo-hooks": "^0.5.0",
        "react-bootstrap": "^1.3.0",
        "react-cookie": "^4.0.3",
        "react-dom": "^16.13.1",
        "react-helmet": "^6.1.0",
        "react-hook-form": "^6.12.2",
        "react-modern-calendar-datepicker": "^3.1.6",
        "react-redux": "^7.2.1",
        "react-router": "^6.0.0-beta.0",
        "react-router-dom": "^6.0.0-beta.0",
        "redux": "^4.0.5",
        "redux-actions": "^2.6.5",
        "undefined": "^0.1.0",
        "winston": "^3.3.3"
    },
    "jest": {
        "roots": [
            "<rootDir>/src"
        ],
        "collectCoverageFrom": [
            "src/**/*.{js,jsx,ts,tsx}",
            "!src/**/*.d.ts"
        ],
        "setupFilesAfterEnv": [
            "<rootDir>/src/test/setupTests.ts"
        ],
        "testMatch": [
            "<rootDir>/src/test/**/*.test.{js,jsx,ts,tsx}"
        ],
        "testEnvironment": "jest-environment-jsdom-fourteen",
        "transform": {
            "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
            "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
            "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
        },
        "transformIgnorePatterns": [
            "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
            "^.+\\.module\\.(css|sass|scss)$"
        ],
        "modulePaths": [],
        "moduleNameMapper": {
            "^react-native$": "react-native-web",
            "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
        },
        "moduleFileExtensions": [
            "web.js",
            "js",
            "web.ts",
            "ts",
            "web.tsx",
            "tsx",
            "json",
            "web.jsx",
            "jsx",
            "node"
        ]
    }
}
